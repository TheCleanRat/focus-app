{
  "manifest_version": 3,
  "name": "FocusTracker Extension",
  "version": "1.0.0",
  "description": "Detects YouTube and Instagram tabs and notifies the FocusTracker app.",
  "permissions": ["tabs", "activeTab", "scripting", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://*.instagram.com/*"
  ]
}
